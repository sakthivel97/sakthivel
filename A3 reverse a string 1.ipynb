     "text": [
      "The original string is : 1234abcd\n",
      "The reversed string is : dcba4321 \n"
     ]
    }
   ],
   "source": [
    "def reverse(s):\n",
    "    str = \" \"\n",
    "    for i in s:\n",
    "        str = i + str\n",
    "    return str\n",
    "s = \"1234abcd\"\n",
    "print('The original string is : ',end='')\n",
    "print(s)\n",
    "\n",
    "print('The reversed string is : ',end='')\n",
    "print(reverse(s))"
   ]
