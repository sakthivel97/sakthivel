     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The required output is =  [3, 6, 9, 12, 15, 18, 21]\n"
     ]
    }
   ],
   "source": [
    "l = [1,2,3,4,5,6,7]\n",
    "def triple(num):\n",
    "    return (num*3)\n",
    "output = list(map(triple,l))\n",
    "print(\"The required output is = \",output)"
