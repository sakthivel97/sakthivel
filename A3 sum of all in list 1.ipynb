   "source": [
    "def sum(*args):\n",
    "    sum1=0\n",
    "    for i in args:\n",
    "        sum1 = i + sum1\n",
    "    return sum1\n",
    "print(sum(8,2,3,0,7))\n",
    "print('THE END'.center(70,'#'))"
